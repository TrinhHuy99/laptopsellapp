{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "./src",
        "paths": {
            "actions/*": [
                "actions/*"
            ],
            "api/*": [
                "api/*"
            ],
            "saga/*": [
                "saga/*"
            ],
            "app/*": [
                "app/*"
            ],
            "assets/*": [
                "assets/*"
            ],
            "components/*": [
                "components/*"
            ],
            "contants/*": [
                "contants/*"
            ],
            "pages/*": [
                "pages/*"
            ],
            "reducers/*": [
                "reducers/*"
            ],
